<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.asif.du_crs">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".Sign_Up"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".sign_in"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".signUpselect"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity android:name=".Reservations" />
        <activity
            android:name="com.example.omi.du_crs.ScrollingActivity"
            android:label="@string/title_activity_scrolling"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name="com.example.omi.du_crs.Main2Activity"
            android:label="@string/title_activity_main2" />
        <activity android:name="com.example.omi.du_crs.SearchActivity"></activity>
    </application>

</manifest>